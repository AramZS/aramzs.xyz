    "autoprefixer": "^10.4.2",
    "cross-env": "^7.0.3",
    "html-minifier": "^4.0.0",
    "node-fetch": "^2.6.7",
    "postcss-cli": "^8.3.1",
    "postcss-import": "^14.0.2"
  },
  "dependencies": {
